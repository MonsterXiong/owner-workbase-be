<%#
    formDataCode,
    prop
    自带->绑定字段

    显示名称：label
    占位提示：placeholder

-%>
<%_ if(displayType == 'image'){ -%>
<el-upload with-credentials class="image-uploader" action="#" :http-request="handleUploadFile" accept='.jpg,.png,jpeg,.bmp' :show-file-list="false">
      <img v-if="<%=formDataCode%>.<%=prop%>" :src="<%=formDataCode%>.<%=prop%>" class="image" />
      <i v-else class="el-icon-plus image-uploader-icon"></i>
      </el-upload>
<%_ } -%>