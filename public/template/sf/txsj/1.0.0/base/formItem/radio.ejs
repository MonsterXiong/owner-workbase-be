<%#
    formDataCode,
    prop
    自带->绑定字段

    显示名称：label
    占位提示：placeholder

-%>
<%_ if(displayType == 'radio'){ -%>
<el-radio-group size="mini" v-model="<%=formDataCode%>.<%=prop%>">
          <el-radio v-for="item in <%=prop%>Option" :key="item.id" :label="item.id">{{item.name}}</el-radio>
        </el-radio-group>
<%_ } -%>