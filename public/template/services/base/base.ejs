import { service } from '@/services/http'

/**
 * 关于<%=tableCommon %>的服务
 */
export default class <%=TableName %>Service {
  /**
   * @description 删除<%=tableCommon %>(批量、递归)
   * @returns
   */
  static async delete<%=TableName %>Batch(<%=prikey %>List) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/delete<%=TableName %>Batch', <%=prikey %>List)
  }

  /**
   * @description 导入<%=tableCommon %>模板下载
   * @returns
   */
  static async download<%=TableName %>Template() {
    return service.postFormData('/<%=basePrefix %>/<%=tableName %>/download<%=TableName %>Template', {})
  }

  /**
   * @description 导出<%=tableCommon %>
   * @param {Object} queryCondition 查询条件
   * @returns
   */
  static async export<%=TableName %>(queryCondition) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/export<%=TableName %>', queryCondition, {
      responseType: 'blob',
    })
  }

  /**
   * @description 获取<%=tableCommon %>
   * @param {String} <%=prikey %> <%=tableCommon %>主键
   * @returns
   */
  static async get<%=TableName %>(<%=prikey %>) {
    return service.postQuery('/<%=basePrefix %>/<%=tableName %>/get<%=TableName %>', { <%=prikey %> })
  }

  /**
   * @description 导入<%=tableCommon %>
   * @param {File} file 文件对象
   * @returns
   */
  static async import<%=TableName %>(file) {
    return service.postForm('/<%=basePrefix %>/<%=tableName %>/import<%=TableName %>', { file })
  }

  /**
   * @description 增加<%=tableCommon %>
   * @returns
   */
  static async insert<%=TableName %>(params) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/insert<%=TableName %>', params)
  }

  /**
   * @description 增加<%=tableCommon %>(批量)
   * @returns
   */
  static async insert<%=TableName %>Batch(<%=tableName %>List) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/insert<%=TableName %>Batch', <%=tableName %>List)
  }

  /**
   * @description 查询<%=tableCommon %>列表结果
   * @param {Object} queryCondition 查询条件
   * @returns
   */
  static async query<%=TableName %>(queryCondition) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/query<%=TableName %>', queryCondition)
  }

  /**
   * @description 保存<%=tableCommon %>
   * @returns
   */
  static async save<%=TableName %>(<%=tableName %>DTO) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/save<%=TableName %>', <%=tableName %>DTO)
  }

  /**
   * @description 保存<%=tableCommon %>(批量)
   * @returns
   */
  static async save<%=TableName %>Batch(params) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/save<%=TableName %>Batch', params)
  }

  /**
   * @description 修改<%=tableCommon %>
   * @returns
   */
  static async update<%=TableName %>(params) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/update<%=TableName %>', params)
  }

  /**
   * @description 修改<%=tableCommon %>(批量)
   * @returns
   */
  static async update<%=TableName %>Batch(<%=tableName %>List) {
    return service.post('/<%=basePrefix %>/<%=tableName %>/update<%=TableName %>Batch', <%=tableName %>List)
  }
}
