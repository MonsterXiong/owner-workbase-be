<%#
    formDataCode,
    prop
    自带->绑定字段

    显示名称：label
    占位提示：placeholder

-%>
<%_ if (list.length > 0) { -%>
  <% list.forEach(function(field){ -%>
  <%_ if (field.isRequired) { -%>
  <%= field.prop%>: [{ required: true, message: '请输入<%= field.label%>', trigger: 'blur' }],
  <%_ } -%>
  <%_ }); -%>
<%_ } -%>