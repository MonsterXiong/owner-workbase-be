import { routesConstant } from '@/router/routeConstant'

export const menuList = [
  <%_ list.forEach(function(item){ -%>
  {
    id: '<%= item.id %>',
    parent: '<%= item.parentId %>',
    code: '<%= item.code %>',
    name: '<%= item.name %>',
    icon: '<%= item.icon %>',
    menuType: '<%= item.menuType %>',
    <%_if(item.menuType=='page' ) { -%>
    menuParams: routesConstant.<%= item.code %>.path,
    <%_ } -%>
    sort: '<%= item.sort %>',
  },
  <%_ }); -%>
]
