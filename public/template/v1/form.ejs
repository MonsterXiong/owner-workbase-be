  <el-form-item label="<%=label%>" prop="<%=prop%>">
      <%_if (displayType == 'text') { -%>
        <el-input v-model.trim="formData.<%=prop%>" placeholder="请输入<%=label%>"></el-input>
      <%_ } -%>
      <%_  if(displayType=='textarea' ){ -%>
        <el-input v-model.trim="formData.<%=prop%>" type="textarea"></el-input>
      <%_ } -%>
      <%_if (displayType == 'select') { -%>
        <el-select v-model="formData.<%=prop%>" clearable placeholder="请选择<%=label%>">
          <%_if (selectType == 'dict') { -%>
          <el-option v-for="item in $root.getEnumByCode(dictCategory.<%=dictCode %>) || []" :key="item.code" :label="item.name" :value="item.code"></el-option>
          <%_ }else if (selectType == 'entity') { -%>
          <el-option v-for="item in <%=prop%>Option || []" :key="item.<%=entityKey%>" :label="item.<%=entityLabel%>" :value="item.<%=entityKey%>"></el-option>
          <%_ } -%>
        </el-select>
      <%_ } -%>
  </el-form-item>