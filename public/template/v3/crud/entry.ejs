<template>
  <TableLayout>
    <%_if(hasToolbar){-%>
    <template #query>
      <<%=pageName%>Query
      <%_if (queryBtnList.length){-%>
        <%_ queryBtnList.forEach(function(btn){ -%>
        @<%=btn.code%> ="<%=btn.code%>"
        <%_ }); -%>
      <%_}-%>
      <%_if (toolbarBtnList.length){-%>
        <%_ toolbarBtnList.forEach(function(btn){ -%>
        @<%=btn.code%> ="<%=btn.code%>"
        <%_ }); -%>
      <%_}-%>
        @onQuery="queryTableData"
        @onReset="onReset"
        :queryForm.sync="queryForm"
      ></<%=pageName%>Query>
    </template>
    <%_}-%>

    <<%=pageName%>Table
      :pageInfo.sync="pageInfo"
      :tableData="tableData"
      :total="total"
      <%_if(hasDeleteBatch){-%>
      @selection-change="onSelectionChange"
      <%_}-%>
      <%_if (tableBtnList.length){-%>
      <%_ tableBtnList.forEach(function(btn){ -%>
      @<%=btn.code%>="<%=btn.code%>"
      <%_ }); -%>
      <%_}-%>
    ></<%=pageName%>Table>
    <%if(hasUpdate){%>
    <<%=pageName%>UpdateDialog ref="updateDialogRef" @refresh="queryTableData"></<%=pageName%>UpdateDialog>
    <%_}-%>
    <%_if(hasAdd){-%>
    <<%=pageName%>AddDialog ref="addDialogRef" @refresh="queryTableData"></<%=pageName%>AddDialog>
    <%_}-%>
  </TableLayout>
</template>